{
  "name": "org",
  "version": "0.0.16",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "MODE=development nx run-many -t _serve",
    "build": "nx run-many -t build",
    "clear": "nx run-many -t clear",
    "serve": "MODE=production nx run-many -t _serve",
    "stop": "nx run-many -t _stop",
