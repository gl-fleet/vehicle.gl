  },
  "workspaces": [
    "core/*",
    "api/*",
    "app/*"
  ],
  "dependencies": {
    "ubin": "upacks/ubin#1.0.7",
    "ucan": "upacks/ucan#1.0.4",
    "unet": "upacks/unet#1.1.14",
    "utils": "upacks/utils#1.1.4",
    "uweb": "upacks/uweb#1.1.14"
  },
  "devDependencies": {
    "nx": "16.7.4"
  }
}
